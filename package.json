{
    "name": "txml",
    "version": "6.0.0",
    "description": "fastest XML DOM Parser for node/browser/worker",
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "scripts": {
        "build": "rollup -c && npm run types:copy",
        "types:copy": "cp src/*.d.ts dist/",
        "test": "node --test test/test-*.js",
        "prepublishOnly": "npm run build && npm test"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "MIGRATION.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TobiasNickel/tXml.git"
    },
    "browser": {
        "fs": false
    },
    "keywords": [
        "xml",
        "parser",
        "dom",
        "document",
        "small",
        "fast",
        "simple"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs",
            "default": "./dist/index.mjs"
        },
        "./txml": {
            "types": "./dist/tXml.d.ts",
            "import": "./dist/txml.mjs",
            "require": "./dist/txml.cjs",
            "default": "./dist/txml.mjs"
        },
        "./transform-stream": {
            "types": "./dist/transformStream.d.ts",
            "import": "./dist/transformStream.mjs",
            "require": "./dist/transformStream.cjs",
            "default": "./dist/transformStream.mjs"
        },
        "./package.json": "./package.json"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "author": "Tobias Nickel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/TobiasNickel/tXml/issues"
    },
    "types": "dist/index.d.ts",
    "homepage": "https://github.com/TobiasNickel/tXml#readme",
    "devDependencies": {
        "rollup": "^4.24.0",
        "@rollup/plugin-terser": "^0.4.4",
        "typescript": "^5.6.0"
    }
}